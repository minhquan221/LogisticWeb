<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en" class="no-js">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="/Content/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/assets/plugins/bootstrap/css/bootstrap-responsive.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/assets/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="/Content/assets/css/style-metro.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/spinkit/spinkit.css" rel="stylesheet" />
    <link href="/Content/assets/css/style.css" rel="stylesheet" type="text/css" />
    <link href="/Content/assets/css/style-responsive.css" rel="stylesheet" type="text/css" />
    <link href="/Content/assets/css/themes/light.css" rel="stylesheet" type="text/css" id="style_color" />
    <link href="/Content/assets/plugins/uniform/css/uniform.default.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/fontroboto.css" rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <link href="~/Content/sweetalert2.css" rel="stylesheet" />
    <link href="~/Scripts/Common/notifications.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    @RenderSection("styles", required: false)
    <!-- END PAGE LEVEL STYLES -->
    <link rel="shortcut icon" href="/favicon.ico" />
</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="page-header-fixed page-footer-fixed">
    <!-- BEGIN SAMPLE PORTLET CONFIGURATION MODAL FORM-->
    <!-- END SAMPLE PORTLET CONFIGURATION MODAL FORM-->
    <!-- BEGIN HEADER -->
    @*@{Html.RenderAction("Header", "Common"); }*@

    <div class="header navbar navbar-inverse navbar-fixed-top">
        <!-- BEGIN TOP NAVIGATION BAR -->
        <div class="navbar-inner">
            <div class="container-fluid">
                <!-- BEGIN LOGO -->
                <a class="brand" href="/">
                    <div class="logo-str">
                        <img src="~/Content/logo-trans.png" style="width: 41px;margin-top: -10px;float: left;" />
                        <span class="spanlogo">LAMIPEL ERP</span>
                    </div>
                </a>
                <!-- END LOGO -->
                <!-- BEGIN RESPONSIVE MENU TOGGLER -->
                <a href="javascript:;" class="btn-navbar collapsed" data-toggle="collapse" data-target=".nav-collapse">
                    <img src="/Content/assets/img/menu-toggler.png" alt="" />
                </a>
                <!-- END RESPONSIVE MENU TOGGLER -->
                <!-- BEGIN TOP NAVIGATION MENU -->
                <ul class="nav pull-right">

                    <!-- BEGIN NOTIFICATION DROPDOWN -->
                    @Html.Action("NotificationBar", "Notification")

                    <!-- END NOTIFICATION DROPDOWN -->
                    <!-- BEGIN INBOX DROPDOWN -->
                    @*@Html.Action("InboxMail", "Notification")*@

                    <!-- END INBOX DROPDOWN -->
                    <!-- BEGIN TODO DROPDOWN -->
                    @*@Html.Action("ToDoNotification", "Notification")*@

                    <!-- END TODO DROPDOWN -->
                    <!-- BEGIN USER LOGIN DROPDOWN -->
                    @{
                        var User = LogisticWeb.Common.LoginUser.Current;
                        if (User != null && !User.ISADMIN)
                        {
                            <li class="dropdown user">
                                <a href="/container" style="padding: 2px 15px 10px !important;">
                                    <img style="height:40px;" src="/Content/assets/img/DONGHANG.png" />
                                </a>
                            </li>
                            <li class="dropdown user">
                                <a href="/shippingmarkPrint" style="padding: 2px 15px 10px !important;">
                                    <img style="height:40px;" src="/Content/assets/img/TAOPALLET.png" />
                                </a>
                            </li>
                            <li class="dropdown user">
                                <a href="/resa" style="padding: 2px 15px 10px !important;">
                                    <img style="height:40px;" src="/Content/assets/img/PHIEUSX.png" />
                                </a>
                            </li>
                            <li class="dropdown user">
                                <a href="/pallet" style="padding: 2px 15px 10px !important;">
                                    <img style="height:40px;" src="/Content/assets/img/PALLETLIST.png" />
                                </a>
                            </li>
                        }
                    }
                    <li class="dropdown user">
                        <a href="/importpkl" style="padding: 2px 15px 10px !important;">
                            <img style="height:40px;" src="/Content/logo-warehouse.png" />
                        </a>
                    </li>


                    @Html.Action("ProfileHeaderUser", "Common")

                    <!-- END USER LOGIN DROPDOWN -->
                    <!-- END USER LOGIN DROPDOWN -->
                </ul>
                <!-- END TOP NAVIGATION MENU -->
            </div>
        </div>
        <!-- END TOP NAVIGATION BAR -->
    </div>


    <!-- END HEADER -->
    <!-- BEGIN CONTAINER -->

    @RenderBody()

    <!-- END CONTAINER -->
    <!-- BEGIN FOOTER -->
    @*@{Html.RenderAction("Footer", "Common"); }*@
    <div class="footer">
        <div class="footer-inner">
            2019 &copy; Lamipel Viet Nam LLC - Nhon Trach III IP., Hiep Phuoc, Nhon Trach, Dong Nai, Vietnam.
        </div>
        <div class="footer-tools">
            <span class="go-top">
                <i class="icon-angle-up"></i>
            </span>
        </div>
    </div>

    <div id="maskBoxes" class="" tabindex="-1" style="display: none;">
        <div id="maskBoxesFake"></div>
        <div class="maskBoxeContent">
            <div class="col-12">
                <div class="sk-wave">
                    <div class="sk-rect sk-rect1"></div>
                    <div class="sk-rect sk-rect2"></div>
                    <div class="sk-rect sk-rect3"></div>
                    <div class="sk-rect sk-rect4"></div>
                    <div class="sk-rect sk-rect5"></div>
                </div>
            </div>
            <span id="msgLoading"></span>
        </div>
    </div>

    <!-- END FOOTER -->
    <!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->
    <!-- BEGIN CORE PLUGINS -->
    <script src="/Content/assets/plugins/jquery-1.10.1.min.js" type="text/javascript"></script>
    <script src="/Content/assets/plugins/jquery-migrate-1.2.1.min.js" type="text/javascript"></script>
    <!-- IMPORTANT! Load jquery-ui-1.10.1.custom.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->
    <script src="/Content/assets/plugins/jquery-ui/jquery-ui-1.10.1.custom.min.js" type="text/javascript"></script>
    <script src="/Content/assets/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/Common/popper.js"></script>
    <script src="/Content/assets/plugins/bootstrap-hover-dropdown/twitter-bootstrap-hover-dropdown.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Scripts/alert/sweetalert2.all.min.js"></script>
    <script src="~/Scripts/Common/notifications.js"></script>
    <script src="~/Scripts/jquery.cookie.js"></script>
    <script type="text/javascript" src="~/Scripts/Common/common.js"></script>
    <script src="~/Scripts/Common/allcontrols.js"></script>
    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        var usString = '@Newtonsoft.Json.JsonConvert.SerializeObject(LogisticWeb.Common.LoginUser.Current)';
        var socketIp = '@LogisticWeb.Common.Constants.ServerNodeIP';
        var loginUser = null;
        if (usString != '' && usString !== undefined) {
            loginUser = JSON.parse(usString.replace(/(&quot\;)/g, "\""));
        }
        $(document).ready(function () {
            GenerateSiteMap();
        });
    </script>
    <script src="~/Scripts/Common/socket-io.min.js"></script>
    <script src="~/Scripts/Common/connectsocket.js"></script>
    <!-- END JAVASCRIPTS -->
</body>
<!-- END BODY -->
</html>